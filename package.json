  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.8.0",
    "@types/string-similarity": "^4.0.0",
    "@unocss/reset": "^0.56.4",
    "@unocss/transformer-directives": "^0.54.1",
